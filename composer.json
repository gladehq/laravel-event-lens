{
    "name": "gladehq/laravel-event-lens",
    "description": "Deep observability for Laravel Events and Listeners with execution tracing and waterfall visualization.",
    "keywords": ["laravel", "events", "monitoring", "observability", "performance"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dulitha Rajapaksha",
            "email": "dulithamahishka94@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "GladeHQ\\LaravelEventLens\\": "src/",
            "GladeHQ\\LaravelEventLens\\Database\\": "database/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GladeHQ\\LaravelEventLens\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GladeHQ\\LaravelEventLens\\EventLensServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "prefer-stable": true
}
